{
  "name": "@krisdages/d3-drag",
  "version": "3.0.0",
  "description": "Drag and drop SVG, HTML or Canvas using mouse or touch input.",
  "homepage": "https://d3js.org/d3-drag/",
  "repository": {
    "type": "git",
    "url": "https://github.com/d3/d3-drag.git"
  },
  "keywords": [
    "d3",
    "d3-module",
    "drag",
    "behavior",
    "interaction"
  ],
  "license": "ISC",
  "author": {
    "name": "Mike Bostock",
    "url": "https://bost.ocks.org/mike"
  },
  "type": "module",
  "files": [
    "dist",
    "src/**/*.js"
  ],
  "module": "src/index.js",
  "main": "dist/d3-drag.min.cjs",
  "jsdelivr": "dist/d3-drag.min.js",
  "unpkg": "dist/d3-drag.min.js",
  "exports": {
    "umd": "./dist/d3-drag.min.js",
    "require": "./dist/d3-drag.min.cjs",
    "default": "./src/index.js"
  },
  "sideEffects": false,
  "dependencies": {
    "d3-dispatch": "npm:@krisdages/d3-dispatch@*",
    "d3-selection": "npm:@krisdages/d3-selection@*"
  },
  "devDependencies": {
    "eslint": "7",
    "mocha": "9",
    "rollup": "2",
    "rollup-plugin-terser": "7"
  },
  "scripts": {
    "test": "mocha 'test/**/*-test.js' && eslint src test",
    "prepublishOnly": "rm -rf dist && yarn test && rollup -c"
  },
  "engines": {
    "node": ">=12"
  }
}
